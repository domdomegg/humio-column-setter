{
  "name": "Humio Column Setter",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Set columns and fields automatically in Humio",
  "homepage_url": "http://github.com/domdomegg/humio-column-setter",
  "icons": {
    "16": "img/icon16.png",
    "19": "img/icon19.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "512": "img/icon512.png"
  },
  "options_page": "options/index.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://cloud.humio.com/*/search",
        "*://cloud.humio.com/*/search?query*"
      ],
      "js": [
        "inject/injector.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inject/script.js"
  ]
}